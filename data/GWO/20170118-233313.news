known"},"FSRVY":{"hasError":false,"error":"none","size":"1x1"}},"_newPageRendered":false,"checkForModalClose":false,"nukeAds":["LREC","LREC2","LREC3","LREC-9","LREC2-9","LREC3-9","MAST","LDRB","LDRB2-1","LDRB2-2","MON-1","SPL"],"adfetchTimeout":null,"_childCompositeReady":{"Col2-5-DockedAds":true}},"VideoStore":{"videos":{},"_cdnBase":"https:\u002F\u002Fs.yimg.com"},"ContentStore":{"currentContentIds":{"default":"388706d3-616a-33dd-ba6c-65ed6055958d"},"relatedUuids":[],"uuidMap":{"388706d3-616a-33dd-ba6c-65ed6055958d":{"ad_meta":{"spaceid":"1183300001","site_attribute":"ticker=\"GWO.TO\" ctopid=\"1542500;1577000;15103489\" hashtag=\"1542500;1577000;15103489\" rs=\"lmsid:a077000000CyFqoAAF;revsp:streetevents;lpstaid:388706d3-616a-33dd-ba6c-65ed6055958d;pct:story\"","site":"finance","region":"US","lang":"en-US","isSupplySegment":"false"},"spaceId":"1183300001","entities":[{"term":"YCT:001000298","score":"0.982196","label":"Finance"},{"term":"YCT:001000323","score":"0.981618","label":"Investment & Company Information"},{"term":"YCT:001000327","score":"0.871429","label":"Company Earnings"},{"term":"YCT:001000298","score":"1.0","label":"Finance"},{"term":"YCT:001000298","score":"0.982196","label":"Finance"},{"term":"YCT:001000323","score":"0.981618","label":"Investment & Company Information"},{"term":"YCT:001000327","score":"0.871429","label":"Company Earnings"}],"liveCoverageEventId":null,"body":[{"type":"text","content":"Q3 2016 Great-West Lifeco Inc Earnings Call","length":43,"tagName":"p"},{"type":"text","content":"WINNIPEG Nov 4, 2016 (Thomson StreetEvents) -- Edited Transcript of Great-West Lifeco Inc earnings conference call or presentation Thursday, November 3, 2016 at 7:30:00pm GMT","length":174,"tagName":"p"},{"type":"text","content":"TEXT version of Transcript","length":26,"tagName":"p"},{"type":"text","content":"================================================================================","length":80,"tagName":"p"},{"type":"text","content":"Corporate Participants","length":22,"tagName":"p"},{"type":"text","content":"================================================================================","length":80,"tagName":"p"},{"type":"text","content":"* Paul Mahon","length":12,"tagName":"p"},{"type":"text","content":"Great-West Lifeco Inc. - President, CEO","length":39,"tagName":"p"},{"type":"text","content":"* Garry MacNicholas","length":19,"tagName":"p"},{"type":"text","content":"Great-West Lifeco Inc. - EVP, CFO","length":33,"tagName":"p"},{"type":"text","content":"* Arshil Jamal","length":14,"tagName":"p"},{"type":"text","content":"Great-West Lifeco Inc. - President, COO Europe","length":46,"tagName":"p"},{"type":"text","content":"* Bob Reynolds","length":14,"tagName":"p"},{"type":"text","content":"Great-West Lifeco Inc. - President, CEO Great-West Lifeco US","length":60,"tagName":"p"},{"type":"text","content":"* Stefan Kristjanson","length":20,"tagName":"p"},{"type":"text","content":"Great-West Lifeco Inc. - President, COO Canada","length":46,"tagName":"p"},{"type":"text","content":"================================================================================","length":80,"tagName":"p"},{"type":"text","content":"Conference Call Participants","length":28,"tagName":"p"},{"type":"text","content":"================================================================================","length":80,"tagName":"p"},{"type":"text","content":"* Steve Theriault","length":17,"tagName":"p"},{"type":"text","content":"Dundee Capital Markets - Analyst","length":32,"tagName":"p"},{"type":"text","content":"* Gabriel Duchaine","length":18,"tagName":"p"},{"type":"text","content":"Canaccord Genuity - Analyst","length":27,"tagName":"p"},{"type":"text","content":"* Peter Routledge","length":17,"tagName":"p"},{"type":"text","content":"National Bank Financial - Analyst","length":33,"tagName":"p"},{"type":"text","content":"* Doug Young","length":12,"tagName":"p"},{"type":"text","content":"Desjardins Capital Markets - Analyst","length":36,"tagName":"p"},{"type":"text","content":"* Sumit Malhotra","length":16,"tagName":"p"},{"type":"text","content":"Scotia Capital - Analyst","length":24,"tagName":"p"},{"type":"text","content":"* Mario Mendonca","length":16,"tagName":"p"},{"type":"text","content":"TD Securities - Analyst","length":23,"tagName":"p"},{"type":"text","content":"* Darko Mihelic","length":15,"tagName":"p"},{"type":"text","content":"RBC Capital Markets - Analyst","length":29,"tagName":"p"},{"type":"text","content":"* Tom Mackinnon","length":15,"tagName":"p"},{"type":"text","content":"BMO Capital Markets - Analyst","length":29,"tagName":"p"},{"type":"text","content":"* Paul Holden","length":13,"tagName":"p"},{"type":"text","content":"CIBC - Analyst","length":14,"tagName":"p"},{"type":"text","content":"================================================================================","length":80,"tagName":"p"},{"type":"text","content":"Presentation","length":12,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [1]","length":12,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Good morning, ladies and gentlemen, and welcome to the Great-West Lifeco Inc.'s third quarter 2016 conference call. I would now like to turn the meeting over to Mr. Paul Mahon. Please go ahead, Mr. Mahon.","length":204,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [2]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you very much, Paul. Good afternoon. Welcome to Great-West Lifeco's third quarter of 2016 conference call. I'm joined today by Garry MacNicholas, Executive Vice President and Chief Financial Officer; Bob Reynolds, President and Chief Executive Officer Great-West Lifeco US; Arshil Jamal, President and Chief Operating Officer Europe; Mark Corbett, Lifeco's Chief Investment Officer; and Stefan Kristjanson, President and Chief Operating Officer Canada.","length":458,"tagName":"p"},{"type":"text","content":"I'd like to actually welcome Stefan to his first analyst call as the leader of our Canadian operations. Stefan, who took over this role beginning in August, has a strong leadership track record, including our Canadian group operations' accountability and the very successful Irish Life integration. Welcome, Stefan. There are also a number of other senior officers available on the call to respond to specific questions as required.","length":432,"tagName":"p"},{"type":"text","content":"I will review the highlights of Lifeco's third quarter results, including headlines for our Canadian, US, and European business operations, and then Garry MacNicholas will provide a more detailed financial review. And after our prepared remarks, we going to open up the lines to questions.","length":289,"tagName":"p"},{"type":"text","content":"Before we start, I'll draw your attention to our cautionary notes regarding forward-looking information and non-IFRS financial measures on slide two. These cautionary notes will apply to the discussion you'll hear today as well as to the presentation material that we've provided.","length":280,"tagName":"p"},{"type":"text","content":"Turning to slide four, earlier today Lifeco reported its third quarter earnings and also declared a quarterly dividend on common shares of CAD0.346 per share. And that's unchanged from the prior quarter.","length":203,"tagName":"p"},{"type":"text","content":"Earnings this quarter were CAD674 million, or CAD0.68 per share. These headline figures are in line with last quarter, but down year-over-year. Q3 2016 earnings were negatively impacted by the movement of sterling in the past 12 months. The currency impact this quarter compared to the same quarter last year was CAD45 million. On a constant currency basis, Q3 2016 earnings were level with last year's results. Garry will cover earnings in more detail in his section.","length":468,"tagName":"p"},{"type":"text","content":"Capital strength and flexibility continues, with our MCCSR ratio at 227%. The decline in MCCSR ratio from last quarter reflects the deployment of capital for growth, including the acquisitions of Aviva Health and GloHealth in Ireland, which were internally funded.","length":264,"tagName":"p"},{"type":"text","content":"In addition, we deployed CAD170 million of capital through share buybacks in the quarter. There remains CAD700 million of liquidity at the Lifeco holding company level that is not reflected in the MCCSR ratio I quoted. We are well positioned, given this capital, to continue pursuing our strategic investments, both organically and through M&amp;A activity.","length":357,"tagName":"p"},{"type":"text","content":"Moving on to slide five, our Canadian sales increased by 6% year-over-year, driven by very strong individual insurance and group wealth management sales. This was partially offset by softer mutual fund and segregated fund sales, consistent with the market.","length":256,"tagName":"p"},{"type":"text","content":"US sales were down slightly year-over-year, but still robust. At Empower, the business mix was skewed to the more profitable smaller case sales, which were up 20% year-to-date. Large and mega fund sales, which tend to be lumpy, were lighter in the period as compared to a year ago. I would note that Empower continues to have a very strong pipeline across all segments.","length":369,"tagName":"p"},{"type":"text","content":"Putnam's retail mutual funds continue to be impacted by the general industry outflows for active equity managers. However, this was balanced by strong institutional sales and positive institutional net flows again this quarter.","length":227,"tagName":"p"},{"type":"text","content":"Europe continues to see steady sales growth. Excluding the impact of a very large institutional sale in Q3 2015, sales were up 9% year-over-year. I would note particular strength in retail payout annuity sales, and in the UK bulk annuity in both -- and in bulk annuity in both the UK and Ireland. Pricing margins continue to be very strong and attractive in this business.","length":372,"tagName":"p"},{"type":"text","content":"Referring to slide six, overall expense growth continues to moderate, reflecting a balance of investment in our businesses and operational expense discipline. This quarter includes CAD21 million of expenses related to both operations and integration at our newly formed Irish Life Health business unit.","length":302,"tagName":"p"},{"type":"text","content":"US expense growth reflects the increase in participants at Empower from 7.5 million in Q3 2015 to over 8 million today, as well as continuing integration efforts and platform enhancements. The integration continues to be on track to conclude by Q2 2017, with just CAD5 million to CAD10 million of spend on integration and enhancement of the platform remaining.","length":360,"tagName":"p"},{"type":"text","content":"The higher Empower expenses were partially offset by lower variable expenses at Putnam. The increased expense levels from Q2 and year-over-year in Europe were primarily due to Irish Life Health, as noted earlier.","length":212,"tagName":"p"},{"type":"text","content":"Turning to slide seven, fee income was 2% higher year-over-year against the generally positive backdrop of higher equity markets in Canada, the US, and the UK, but lower in Europe. I would note that our fee income results in each business segment are impacted by their mix of assets, whether that's equity versus fixed income, retail versus institutional, and also based on the geographic distribution of their assets.","length":418,"tagName":"p"},{"type":"text","content":"As we look at the segment's results, Canada fee income increased 3%, due mostly to higher equity market levels. Retail net flows continue to be positive in Canada, but overall flows were negative due to a withdrawal from a low margin group retirement plan.","length":256,"tagName":"p"},{"type":"text","content":"In the US, while average assets levels and associated fees increased at Empower, they were offset by lower mutual fund AUM and performance fees at Putnam, resulting in overall lower fee income in the US segment. Fee and other income is up significantly in the European segment, driven by growth in fee income on assets under management, growth in other income related to our Irish Life Health business, as well as fees on a legacy book block of business at Irish Life, where fees are linked to in-period changes in equity market levels.","length":536,"tagName":"p"},{"type":"text","content":"Turning to slide eight, Brexit continues to be an area of global focus and uncertainty, as speculation over trade negotiations and potential outcomes continues. And as we previously indicated, our UK business is domestic in nature. As such, the impact on our business has largely been constrained due to changes in exchange rates.","length":330,"tagName":"p"},{"type":"text","content":"In Q3, our UK businesses reported strong overall results, including the benefit of longevity basis changes. As previously mentioned, the weaker sterling impacted Lifeco's earnings by approximately CAD45 million when compared to Q3 2015 exchange rates.","length":251,"tagName":"p"},{"type":"text","content":"In addition, with the UK property market having stabilized, we removed the 3.5% valuation adjustment that we had applied to our properties in the general fund. We also reopened our unit-linked property funds to redemptions.","length":223,"tagName":"p"},{"type":"text","content":"So, to sum up, while there remains a lot of uncertainty regarding how Brexit will impact the UK, and in particular companies that have used the UK to passport into the EU, our businesses there are performing very well. We remain confident in the strength of our UK balance sheet and our UK growth prospects.","length":307,"tagName":"p"},{"type":"text","content":"Referring to slide nine, in Q3 we launched our Irish Life Health business combining the acquired operations of Aviva Health and GloHealth, creating the number three player in the Irish health insurance market with a 21% market share. The integration is off to a very good start, with completion expected in 18 months.","length":317,"tagName":"p"},{"type":"text","content":"And of the expected EUR 16 million of integration costs, EUR 8 million have been expensed in Q3. The integration is expected to generate EUR 16 million of annual run rate synergies. In Q3, as part of the transaction, we booked a CAD24 million fair value adjustment to write-up the value of our existing interest in GloHealth.","length":325,"tagName":"p"},{"type":"text","content":"I'm now going to turn the call over the Garry to review Lifeco's financial results in greater detail. Garry?","length":108,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [3]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you, Paul. Starting with slide 11, operating earnings in the third quarter were CAD674 million, or CAD0.68 per share versus CAD0.72 a year ago. Earnings this quarter were in line with the prior quarter, but down 6% year-over-year, largely impacted by currency and also a higher effective tax rate.","length":303,"tagName":"p"},{"type":"text","content":"Canada's earnings were down 11% from the third quarter of 2015, largely due to a number of factors, including higher strain on individual insurance sales, less favorable morbidity and policyholder behavior experience in individual insurance, higher operating expenses for wealth management business, and a tax benefit in the group division that did not recur this year.","length":369,"tagName":"p"},{"type":"text","content":"In the US, earnings improved from Q2, but were down from the prior quarter -- prior year quarter, largely due to lower investment gains and increased strategic expenses at Great West Financial.","length":193,"tagName":"p"},{"type":"text","content":"At Empower, both fee income and operating expenses increased year-over-year due to business growth, as the number of participants has grown 7%, from 7.5 million to over 8 million in the past year.","length":196,"tagName":"p"},{"type":"text","content":"Putnam's fee income declined due to a reduction in retail mutual funds and unfavorable performance fees. This was offset by improved investment income on seed capital, a decline in variable expenses, and a one-time recovery of $8 million.","length":238,"tagName":"p"},{"type":"text","content":"Europe's operating earnings of CAD313 million remained strong, up 20% on a constant currency basis, with a higher contribution from basis changes this quarter offsetting unfavorable mortality experience in the life reinsurance division. As noted earlier, there was a significant currency impact due to the decline in sterling compared to Q3 last year. But also, when compared to Q2, there was a CAD19 million adverse currency impact since average currency translation rates last quarter were largely unaffected by Brexit, as it occurred right near the end of the quarter.","length":571,"tagName":"p"},{"type":"text","content":"Turning to slide 12, and as a reminder, the source of earnings categories above the line are shown pre-tax. Lifeco's year-over-year expected profit decreased CAD75 million from 2015. This was mainly driven by the US, largely a reduction in mutual funds' mix and unfavorable performance fees for Putnam, which was CAD42 million lower in expected profit from Q3 2015, although I would note the bottom line at Putnam was actually a modest improvement year-over-year.","length":463,"tagName":"p"},{"type":"text","content":"In Europe, the decline primarily reflects the decline in sterling from Q3 last year. Expected profit increased CAD28 million, or 4% sequentially from Q2, mainly due to anticipated seasonality in Canadian and UK group businesses.","length":228,"tagName":"p"},{"type":"text","content":"New business strain increased to CAD77 million, largely due to higher business volumes for reinsurance products with upfront strain, and similarly, for higher volumes in Canadian individual insurance new business.","length":213,"tagName":"p"},{"type":"text","content":"Experience gains of CAD11 million were lower this quarter than the prior quarter and prior year. Investment activity was strong, contributing CAD75 million to earnings, but this was offset by CAD64 million of other experience loss. Life reinsurance results within the Europe segment were notably weaker this quarter, with a CAD26 million experience loss.","length":354,"tagName":"p"},{"type":"text","content":"Group Canadian disability results, while improving steadily from Q4, Q1, and Q2 as a result of pricing actions undertaken, were still below expectations for this quarter and also lower than Q3 last year, which happened to be a very strong quarter.","length":247,"tagName":"p"},{"type":"text","content":"Assumption updates and other management actions resulted in the release of CAD223 million this quarter, compared to CAD129 million last quarter and CAD127 million last year. Over CAD200 million was related to updated annuitant longevity assumptions in all segments, but with the lion's share of those arising in sterling on the large UK payout annuity book, which of course increased the currency impact that we've seen.","length":420,"tagName":"p"},{"type":"text","content":"The earnings on surplus of CAD34 million were CAD44 million higher than last year. Following the transaction to assume control of GloHealth during the quarter, the company reported a fair value gain which increased net earnings by CAD24 million. There were also gains on seed capital portfolios in Canada and at Putnam compared to losses in the prior year period.","length":363,"tagName":"p"},{"type":"text","content":"All this comes together in net income before tax shown in the middle of the page. Year-over-year this is down 2%, but an improvement quarter-over-quarter. Results are up CAD41 million, or 5%. I'd also note here the effective tax rate on shareholder earnings is 15%, up from the 11% rate last year and last quarter due to fewer one-time tax benefits this quarter.","length":362,"tagName":"p"},{"type":"text","content":"Turning to slide 13, Lifeco's uncommitted cash position remained strong at just over CAD700 million, although declining during the quarter as a result of accelerated share buybacks in the period. Our book value per share was CAD19.18, comparable to both the prior quarter and prior year quarter, reflecting a reversal of the portion of US dollar exchange rate impact seen late in 2015 and pension re-measurements at lower interest rates and, of course, the impact of the share buybacks.","length":486,"tagName":"p"},{"type":"text","content":"Return on equity fell 13.8%, reflecting the impact of currency on earnings in this quarter compared to Q3 2015. The impact of FX this quarter, or more specifically the 8% drop in pound sterling, reduced our ROE below 14%.","length":221,"tagName":"p"},{"type":"text","content":"ROEs continue to be strong in Canada and Europe, above our long term targets. The lower return in the US is the combination of ongoing investments in Empower and integration of growth platforms, as well as pressure on margins from lower mutual fund assets and the lack of scale at Putnam. Management is very focused on improving margins and ROE in the US segment, and bringing Lifeco back towards its longer term targets.","length":421,"tagName":"p"},{"type":"text","content":"And finally, on slide 14, we show assets under administration are at CAD1.2 trillion, up 9% on a constant currency basis, driven largely by market performance and fair value gains as well as overall business growth.","length":215,"tagName":"p"},{"type":"text","content":"Paul, that concludes my remarks.","length":32,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [4]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks, Garry. Before we open up the lines to questions, I want to take a few moments to sum up the quarter.","length":108,"tagName":"p"},{"type":"text","content":"Notwithstanding continuing challenging economic conditions, there continues to be a strong focus on driving forward with our strategy, including investing in the businesses and executing targeted acquisitions. Overall, our diversification of businesses continues to serve us well. Across our operations, we are focused on expense efficiencies, balanced with investment in automation and differentiation to ensure we remain competitive.","length":435,"tagName":"p"},{"type":"text","content":"For Putnam, we remain focused on reaching profitability through cost management and driving growth to reach scale in a challenging environment. And importantly, we remain committed and confident in our strategic direction and growth prospects, supported by our capital strength and flexibility.","length":294,"tagName":"p"},{"type":"text","content":"That concludes my formal comments, so I'm now going to turn the line back over to the operator so we can get set up for questions. Thanks.","length":138,"tagName":"p"},{"type":"text","content":"================================================================================","length":80,"tagName":"p"},{"type":"text","content":"Questions and Answers","length":21,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [1]","length":12,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you. We will now take questions from the telephone lines. (Operator instructions.) Steve Theriault, Dundee Capital Markets.","length":129,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Steve Theriault, Dundee Capital Markets - Analyst [2]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks very much. Good afternoon, everyone. So, just first, I don't know if it was Paul or Garry, in your prepared remarks you mentioned CAD24 million of adjustment on the write-up of GloHealth. Is that pre-tax or after-tax, and where can I see that in the source of earnings?","length":276,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [3]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"This is Paul here. I'll refer that over to Garry. Garry?","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [4]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That is -- it's both after-tax and pre-tax just given the nature of the item. And it appears in the earnings on surplus line on the source of earnings.","length":151,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Steve Theriault, Dundee Capital Markets - Analyst [5]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And then just turning to expected profit, down for a couple of consecutive quarters now. You've given some explanation. Putnam's been a driver. It looks like about half of the 10% decline this quarter, but a couple of questions. With AUM up quarter-on-quarter and year-on-year at Putnam, shouldn't we be seeing some improvement in the expected profit from Putnam? I thought that was the case, given prior comments?","length":414,"tagName":"p"},{"type":"text","content":"And secondly, Q4 looks like a pretty difficult comparative quarter. Would you expect we should see expected profit down again in Q4 or are there some positive catalysts you could point us to for next quarter, or is the currency delta too overwhelming?","length":251,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [6]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I'm going to -- there's a lot of things that go into that, including mix of business, but I'll let Garry speak to the expected profit.","length":134,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [7]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Right. And I'll focus my comments on Q3. I can't comment on what's going to happen in Q4. We'll leave that until this call three months from now.","length":145,"tagName":"p"},{"type":"text","content":"On the expected profit -- and we do actually show in the supplemental deck the expected profit for Putnam. And it is somewhat of a job, I mean, putting asset management income into a source of earnings display, although we do it on a kind of separate basis.","length":257,"tagName":"p"},{"type":"text","content":"So, what you're seeing there, while the AUM -- I believe the average AUM was down about 1% from the prior year quarter, the expected profit is down more than that, largely due to the mix of AUM. So, there's lower mutual funds and higher institutional sales. So, it's just the raw fee income item, which go into expected profit, and you see a lower expected profit number.","length":371,"tagName":"p"},{"type":"text","content":"That's not all going on at Putnam. It's just that mix of type of tightened AUM between retail and institutional.","length":112,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Steve Theriault, Dundee Capital Markets - Analyst [8]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. And then last, if I could, for Arshil, can you refresh us on your outlook for reinsurance? Last quarter you sounded pretty positive, but year-to-date your earnings are down. Q3 was the lowest we've seen in a little while. Maybe that's currency. We're just seeing it in Canadian dollars. But a bit of a refresh there would be helpful. Thanks.","length":347,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [9]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"To start with, it's Paul here. On the whole reinsurance side, I think, Jeff -- pardon me, Garry would have referenced the traditional life reinsurance in the US in terms of some of the mortality, just in-period mortality. And I think that was fundamentally the drag.","length":266,"tagName":"p"},{"type":"text","content":"But, Arshil, you may want to add something to that.","length":51,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Arshil Jamal, Great-West Lifeco Inc. - President, COO Europe [10]","length":65,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, that's exactly right. So, the in-period performance, the drop that you're seeing is really driven by unfavorable mortality experience in the US, where on a number of claims basis we're tracking right in line with expected. But this quarter there was an elevated average amount of claim. And that was quite material and came through the results. So, whatever -- that's something like a CAD35 million after-tax year-over-year variance related to the mortality performance.","length":474,"tagName":"p"},{"type":"text","content":"So, we continue to be very optimistic about our reinsurance business, and the currency impact there was very modest. There is some sterling earnings within the reinsurance segment, but there's also lots of US dollar earnings. So, currency was less of an issue for reinsurance. It was really the underlying performance of our mortality -- of all-cause mortality, the bulk of business in the US.","length":393,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Steve Theriault, Dundee Capital Markets - Analyst [11]","length":54,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks very much.","length":17,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [12]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks, Steve.","length":14,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [13]","length":13,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel Duchaine, Canaccord Genuity.","length":36,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel Duchaine, Canaccord Genuity - Analyst [14]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Good afternoon. So, where do I start here? The experience gains line, that's an issue that's hitting your Europe segment quite a bit. And the focus has been on the FX impact weighing on profits there. But if I look at the last two years of experience gains in Europe, between 2012, 2014, you had CAD200 million plus of experience gains a year. That dropped to CAD50 million in 2015, and that's negative now for the full year, including this quarter, in 2016. Can you explain to me what's behind that trend? Because it's a big headwind for your profits outside of just the FX issue.","length":581,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [15]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Arshil, I'll let you just sort of provide a little bit of a backdrop on that. Clearly, we just talked about the in-period issue of reinsurance mortality, but, Arshil, you can provide a little bit more color.","length":207,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Arshil Jamal, Great-West Lifeco Inc. - President, COO Europe [16]","length":65,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. The European businesses are quite diverse, so there's different factors impacting at different times. So, this quarter in particular we've noted the mortality underperformance in our reinsurance segment, and that's coming through experience gains and losses.","length":263,"tagName":"p"},{"type":"text","content":"We had quite a strong investment quarter this time around, with material trading gains that would have contributed favorably. But last quarter we had that impairment on a group of mortgages to a UK retailer that was depressing our investment experience.","length":253,"tagName":"p"},{"type":"text","content":"If you go back three or four years ago coming out of the financial crisis, spreads were very, very wide and we were getting very attractive yield enhancement gains. So, if there's any longer term trend to draw, it's probably that, while we're still getting quite a good contribution from investment experience now, the contribution two or three years ago was very elevated.","length":373,"tagName":"p"},{"type":"text","content":"And then beyond that, there's just been fluctuation across the various lines of business, depending on what the driver is that particular quarter. So, I wouldn?t draw more than that broad inference in any one quarter's performance.","length":231,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel Duchaine, Canaccord Genuity - Analyst [17]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"So, we're looking CAD50 million, CAD60 million a quarter of experience gains a few years ago. My words, but lion's share would have probably come from investment gains. The market's tougher, possibly, to generate these gains today versus a few years ago. So, what's a more realistic level for that business? Because Europe has in the -- not just today versus a few years ago, but it's consistently been one of your bigger sources of experience gains. And I'm just wondering when that could possibly turn.","length":504,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Arshil Jamal, Great-West Lifeco Inc. - President, COO Europe [18]","length":65,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, I think we'd just point you to some of the disclosure we have about the long term performance on the experience gain and management action line combined. So, I'd encourage you to look at those two lines together and not worry as much about the geographic breakdown.","length":270,"tagName":"p"},{"type":"text","content":"So, I think if you look on a long term basis, about 20% to 25% of the overall company's earnings have come from those two items. It's performed in line with that for the nine months of this year. We will see some noise from one segment to the other or whatever. But, yes, they're diversified businesses and the underlying performance continues to be very strong.","length":362,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel Duchaine, Canaccord Genuity - Analyst [19]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I don't want to get too deep in the weeds on the actuarial stuff. That can get pretty dense. But my simple view of things is that if you have recurring experience gains, then eventually that leads to a release of reserves, a management action, right? So, a big positive number, because you have excess reserves, clearly, because you're too conservative.","length":353,"tagName":"p"},{"type":"text","content":"But in this case, we have negative experience, and then still the management actions are a positive item. Does that mean -- or if I look at them in combination, maybe you don't have the experience gains coming back because you're upfronting that profit today. And moreover, because you're maybe bringing your PFADs down, your expected profit will also be lower in the future because you're fronting some of that profitability. Is that not the risk of seeing these big levels of management actions today?","length":503,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [20]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel, what you're talking about is -- it's not quite as thinking about it as the same pool that -- where you have experience gains and management actions coming from it.","length":172,"tagName":"p"},{"type":"text","content":"As Arshil outlined, we're dealing with US traditional life mortality. We're looking at longevity in the UK. We're looking at investment -- we would have had investment impacts due to defaults. So, we're talking about three completely separate and, frankly, unrelated issues of moving parts.","length":290,"tagName":"p"},{"type":"text","content":"So, I don't think you can simplify it and say that those things are all one pool. So, I think it's -- our view is that we have had this generally -- a fairly stable contribution. It'll be volatile from quarter-to-quarter, but a fairly stable contribution from the strength of our book.","length":285,"tagName":"p"},{"type":"text","content":"And I go back to where does that come from. Well, when we do acquisitions, we do it on a conservative approach. When we write new business, we write it with good discipline in our pricing and our underwriting. And over the long term, we believe that this provides for a sort of stability of our outcomes.","length":304,"tagName":"p"},{"type":"text","content":"And so, obviously there's going to be volatility in-period. It'll be different from quarter-to-quarter. But over the long term, that conservatism makes for a strong and stable balance sheet. I don't know if there's anything you'd want to add, Garry.","length":249,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [21]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, I think Arshil covered it well earlier, that relative to, say, the averages three years ago, the spreads were wider. There was more investment gain activity. By looking historically, we could do a few averages for all kinds of disclosures.","length":243,"tagName":"p"},{"type":"text","content":"But those investment gains were, in Europe, a bit higher two or three years ago than they have been today, although still making good contributions. But other parts of the business have picked up, Irish Life and all its contributions has really added. So, you're getting a different mix, and Europe continues to grow.","length":317,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel Duchaine, Canaccord Genuity - Analyst [22]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And this is housekeeping stuff, but can you run me through the one-time items again just that I might have missed here? So, you've got that mark-to-market -- or the fair value gain on that Irish Health business. That was CAD24 million. You had an expense reversal at Putnam of $14 million, so CAD20 million or so pre-tax. Was there anything else anywhere? Like, the tax seemed a little bit low, not as low as Q1 and Q2, but still a little bit on the low side. Was there anything of that nature this quarter?","length":507,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [23]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry, I'd just let you make sure that Gabriel has that -- those numbers right and whether --.","length":94,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [24]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes.","length":4,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [25]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"There's anything else.","length":22,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [26]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"The GloHealth was -- turned out at the CAD24 million. That was correct. The Putnam is -- after-tax is $8 million, so that would be maybe CAD11 million. And then the tax rate is on the -- will be 15%. I wouldn't say that was unusually low at all. It is higher than the comparative quarters by about 4%. They were 11% in the comparators. Earlier this year it was a fair bit lower, so it's -- I'd say that's more in line.","length":418,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Gabriel Duchaine, Canaccord Genuity - Analyst [27]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. Thanks. Have a good afternoon.","length":36,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [28]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you.","length":10,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [29]","length":13,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Peter Routledge, National Bank Financial.","length":41,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Peter Routledge, National Bank Financial - Analyst [30]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I think this would be for Garry. The CAD200 million in reserve release, I think you said CAD200 million plus from updated annuitant longevity assumptions, and that that was mainly in the UK. If I have that right, what exactly did you -- assumption did you change? Did you assume people are going to live shorter?","length":312,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [31]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"In simple terms, that's probably a good way to describe it, but I'll let Garry provide a little bit more color.","length":111,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [32]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I think I'll add in a little bit to it, but in a nutshell that is it. And it's not a lot shorter. What we were reflecting was recent experience both that we've seen in our own book and also that's been seen in industry studies that are done in the UK, using both industry and population data, that the rate of improvement is slowing down.","length":338,"tagName":"p"},{"type":"text","content":"So, mortality -- we're still projecting that not only do you have today's experience, but mortality rates will continue to improve, i.e., will continue to get lower, but they're not going to improve as quickly as we had been assuming. So, we've just slowed down that rate of improvement, and that's what's led to the release in Europe.","length":335,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Peter Routledge, National Bank Financial - Analyst [33]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay, that's clear. And then a broader question on the earlier line of questioning around experience gains versus assumption changes. Speaking for myself, I've always thought of experience gains as reflective of your -- of Great-West's underlying conservatism in managing your balance sheet, and therefore a source of high earnings quality -- of high earnings quality. And speaking for myself, I have always looked at management actions and changes and assumptions as low earnings quality, because I can't bank on it.","length":517,"tagName":"p"},{"type":"text","content":"Your chart on page 23 of the appendix does show quite a bit of stability and experience gains and management actions and assumption changes. But the mix of that green bar at the bottom there has shifted pretty dramatically towards assumption changes. Is it fair or unfair to say that you're -- that the earnings quality from that source is much weaker because it relies more on assumption changes than experience gains?","length":419,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [34]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Well, I'll start out, and I would say I would not characterize it that way. As we said, in the period we're in right now with relatively low interest rates -- well, very low interest rates. We've outlined the fact that we've had a bit of an aberration in the US credit life mortality, Canadian LTD morbidity again, and some of those issues actually -- on the LTD actually relate to interest rates, because interest rates have been falling and it's hard to keep up with pricing.","length":477,"tagName":"p"},{"type":"text","content":"So, those things tend to be a drag in-period. But as you reprice and as you -- as we sort of revert back to the mean on things like our US mortality, we would tend to see experience gains sort of getting back to sort of more normalized. But Garry, I'll let you add to that.","length":273,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [35]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, I think that's -- I think you've hit it on the head there, Paul. First of all, the management actions and basis changes, you said that comes from our approach to setting up whether it's -- how we establish acquisitions, how we establish new business. As we set a business up on our balance sheet, we do so on a prudent basis. And then over time, as results prove out favorably, then we're able to release that. And we've been quite consistent in that.","length":455,"tagName":"p"},{"type":"text","content":"On the experience gains, a lot of that goes to trading activity. While that will fluctuate with opportunities, that has been a steady contributor right throughout the piece. And then mortality\u002Fmorbidity, it does move around. If I think over 2015 and 2016, it's been -- there has been a few tougher quarters in a couple of areas. The reinsurance mortality is one of the larger variances that we've seen in that block. You know it's a large block. The group disability results we've mentioned in Canada. But those are repairable in terms of through pricing actions.","length":563,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [36]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry, I might add, if you think about group health, you need -- not only are they repairable through pricing actions, we see a real cycle, and they tend to be a five and seven year cycle, where you get a widening of margins and a narrowing of margins with economic conditions. So, that would be another dynamic.","length":312,"tagName":"p"},{"type":"text","content":"So, it's not as though there's been a sea change in this where we've got a bit of a down cycle in those morbidity results. We see that in the industry. We'll see a widening of those as everybody takes pricing action. So, it's the layering of various aspects, but this is not -- I don't view this as a systemic shift.","length":316,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [37]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, we had -- I think it was in Q1 2015, I know it's going back a bit, but we had experience gains that were well over CAD200 million in the quarter. It was just a remarkable quarter. And again, it was unusual for its size the other way.","length":237,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [38]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes.","length":4,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [39]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And so, they do move around quite a bit.","length":40,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [40]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. And for context, right now we're dealing in a UK environment post Brexit where you look at the real narrowing of yields. And those, in time, are going to widen again, and we'll see stronger contributions.","length":209,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Peter Routledge, National Bank Financial - Analyst [41]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Hopefully not too abruptly, yes.","length":32,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [42]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes.","length":4,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Peter Routledge, National Bank Financial - Analyst [43]","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"All right. Thank you.","length":21,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [44]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks.","length":7,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [45]","length":13,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Doug Young, Desjardins Capital Markets.","length":39,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Doug Young, Desjardins Capital Markets - Analyst [46]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Hi. Good afternoon. Just I guess my first question is just want to clarify that the -- there was no earnings impact from the fair value adjustment of the 3.5% applied to the UK properties. That didn't flow through to earnings at all. Is that correct?","length":250,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [47]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That is absolutely correct.","length":27,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Doug Young, Desjardins Capital Markets - Analyst [48]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. And then just when I was looking back to the UK, Arshil, I think on a constant currency basis -- I think, Paul, in your comments you talked about expenses being up 17% year-over-year. And I understand that there was an acquisition that was included. If you backed out the acquisition contribution, and again on a constant currency basis, what were your expenses up year-over-year? And what would have been some of the drivers there?","length":438,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [49]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I'm going to let Arshil speak to that, because that was the material increase in expense. But Arshil?","length":101,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Arshil Jamal, Great-West Lifeco Inc. - President, COO Europe [50]","length":65,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. So, adjusted for the acquisitions, both the legal and general company a year ago, as well as the Irish Life Health company in this period and adjusting for currency, expenses would have been absolutely flat year-over-year in Europe.","length":237,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Doug Young, Desjardins Capital Markets - Analyst [51]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. So, that's -- okay, that's fine. And then just the MCCSR on a consolidated level I believe was down 5 points sequentially. And I guess the acquisitions, which maybe I didn't fully factor in, had a 4 point impact, but it was still -- if you excluded that, it was still down. What were some of the drivers that caused that? Was there any money moving from the top to the bottom or, sorry, from the bottom to the top? I'm just trying to understand what some of the drivers would have been.","length":492,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [52]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, I'll let Garry speak to that. Garry?","length":41,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [53]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sure. So, the ratio dropped 5 points in the quarter. Four of that was just the Irish Life Health acquisition. You paid for it and you don't get to count the goodwill. And then it also has requirements that go with the block of business that you've acquired. So, that was 4 points.","length":280,"tagName":"p"},{"type":"text","content":"And then there was another 3 points worth of declines from -- the fair value factors were a couple of points, and the hierarchical market. Again, it's a factor based system, so you can get fair value impacts. And that would have been close to 3 points this quarter.","length":265,"tagName":"p"},{"type":"text","content":"So, absent that, you'd have seen a modest rise, which is what you might see typically from our earnings growth plus our dividends. We often see a point or two rise. But it really was just business as usual, other than the acquisition. And with lower rates, drives up fair values, higher requirements.","length":300,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Doug Young, Desjardins Capital Markets - Analyst [54]","length":53,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay, perfect. That's all for me. Thank you very much.","length":54,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [55]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you.","length":10,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [56]","length":13,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital.","length":31,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [57]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks. Good afternoon. This is probably for Garry to start, just going back to the management actions for a moment. It's a pretty regular line item for the company to see something come through in Europe or Canada, not as common for the US. And the CAD50 million in management action gains this quarter did seem to be a big part of the increase in US profits. Just hoping you can give me some color as to what was the specific change that you made in that business.","length":466,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [58]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit, I agree. That is a good question for Garry. Garry?","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [59]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. There is a couple of things. First off, and it is in the supplemental deck, that account recovery that we noted, the $8 million after-tax, translates to CAD19 million pre-tax.","length":181,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [60]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sorry, Garry. That's the Putnam piece?","length":38,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [61]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I beg your pardon?","length":18,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [62]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That was the Putnam piece that you were talking about?","length":54,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [63]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That was the Putnam piece.","length":26,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [64]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay.","length":5,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [65]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"So, that was CAD19 million. What you're left with then in the US segment is CAD34 million, which interestingly was about the same level last year. That CAD34 million was fairly evenly split between a mortality basis change, just updating our mortality rates in the life book of business, and annuitant longevity, which we updated. So, it was the other gain forecasting a slower rate of improvement at certain ages in the US. A much smaller book in the US, so that contributed. So, they're about half each of the remaining CAD34 million.","length":536,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [66]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And that number, you're saying X the Putnam piece being somewhat flat year-over-year. Is that a -- I'm kind of looking back in time and seeing something every Q3. Is there -- sorry if this has been covered before, but is this some kind of a seasonal factor that comes through every Q3 or not necessarily?","length":304,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [67]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I would characterize it as the rhythm of our reserve basis studies. Typically you enter into the year and you have -- you are doing various studies on a certain cycle. And we tend to try and get those studies completed by Q3. It's sort of -- just when those are done. Sometimes they blend into Q4.","length":297,"tagName":"p"},{"type":"text","content":"But I don't think there's any magic. I think that just tends to be the point in the cycle when we've completed studies. And you're either going to be strengthening or releasing depending on what the outcome of those studies are.","length":228,"tagName":"p"},{"type":"text","content":"Garry, anything more than that?","length":31,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [68]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, just to note that these two were both just the result of regular in-year experience studies. And they typically do come up in Q3, sometimes Q4, occasionally other quarters if something in particular has come up. But Q3 is, like you said, like many companies, is a larger time for those changes.","length":298,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [69]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And a second question, maybe I'll leave it here, it feels like -- going back to investor day and some of the conferences that you folks participated in, the topic of capital deployment is one that has come to the forefront in a much bigger way. And I know specifically in US asset management, there was at least one larger deal conducted in the last little while in which the pricing -- the price point for the buyer did seem to be much more attractive than we've seen in the past.","length":481,"tagName":"p"},{"type":"text","content":"When you think about your active asset management capabilities in the US, you've certainly indicated that you're interested in adding scale there. But as far as the environment is concerned, is this something that the market should be prepared for, that you're actively engaged in something happening on the acquisition from in the near term, or is this still more wait and see, from your perspective?","length":401,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [70]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Well, certainly if we were actively engaged, we wouldn't tell --.","length":65,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [71]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sure.","length":5,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [72]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"We wouldn't telegraph that.","length":27,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [73]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And sorry, I didn't really say that very well. I mean, you have indicated that capital is strong and you want to get bigger in certain businesses. And it does seem like, with earnings down on a year-over-year basis and what you've described as a tougher macro environment, it seems like there's areas of the business that have a definitive need. Is that too strong of a way to put it?","length":384,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [74]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit, I think you've got -- characterized that well, that we are absolutely looking to try and drive scale in Putnam. We would continue to think about opportunities on the Empower side.","length":186,"tagName":"p"},{"type":"text","content":"And if you think about the overall US active management market, you would characterize the market today as there being an oversupply, which typically will drive out consolidation. And as you pointed out, there's a deal that was done more -- or at least has the potential to get done. And I think we're going to see more consolidation, and we want to be a player in that. And that's sort of as far as I would go right now to characterize how active we are.","length":455,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Sumit Malhotra, Scotia Capital - Analyst [75]","length":45,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks for your time.","length":21,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [76]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thanks.","length":7,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [77]","length":13,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Mario Mendonca, TD Securities.","length":30,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Mario Mendonca, TD Securities - Analyst [78]","length":44,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Good afternoon. I sort of want to follow up on the assumption change in the UK. The CAD200 million you referred to, I think you said predominantly longevity assumptions. To build some confidence in these, sort of going to Peter's point about the quality, could you talk about what really drove it. And I think, Garry, you referred to industry improvement in longevity I guess maybe going the other way. But you said there was some industry information that you got, and also you own experience studies. What would be helpful to understand is did this all just play out in the quarter, or is this information that you would have been building up over time but this just happened to be the quarter it was booked in?","length":713,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [79]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Well, I'm going to start, before Garry gets into the gory details as much as he chooses to, is that we're constantly monitoring the progression of mortality and morbidity, but in particular annuitant mortality, the improvement. There's tables that contemplate rates of improvement over time. You'll see a softening of that or a shift in that, but you don't react to that immediately. You're constantly looking at it and monitoring it.","length":434,"tagName":"p"},{"type":"text","content":"So, we approach all these things with a degree of conservatism. We don't sort of build things up and wait for a moment. We're constantly looking ahead and making sure that we're approaching this in a very conservative manner, all these things within the range of accepted actuarial standards. But Garry, I'll let you --.","length":320,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [80]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, I think you're summing it up well there. I'd point out that these studies, and much like our other experience studies, are not -- they're not typically driven by a single event that you respond to in a period.","length":213,"tagName":"p"},{"type":"text","content":"These are -- and Paul is right. We do monitor trends both in our own experience -- we've used five, seven, 10 year studies to monitor our own experience. And those don't tend to move dramatically, but we pick up trends over time. And then there are a number of industry-based and population-based studies in the UK. It's a very active area of study there. So, we're constantly monitoring that research.","length":402,"tagName":"p"},{"type":"text","content":"Again, often this comes up in Q3 because that's a normal cadence of our reviews. But there's no -- there wasn't a single point in time event that happened in Q3 that suddenly drives the change. And our philosophy is to be directionally correct and to not get ahead of -- not get overly ahead of trends or respond to --.","length":319,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [81]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"You might even say we try to --.","length":32,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [82]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"The news of the day.","length":20,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [83]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"We try and go partway to trends and always use a lot caution is the way we would do it.","length":87,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Mario Mendonca, TD Securities - Analyst [84]","length":44,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. So, perhaps just a related question, again on this annuitant mortality, or longevity, rather. Had you seen a number of quarters of experience gains associated with longevity prior to making this change? And if so, over what time period had you seen the experience gains that drove this, drove the release?","length":311,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [85]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I think I'd comment first to say we have seen experience gains in longevity. They're reasonably modest, but they are -- we have seen experience gains over time and reasonably steady. I mean, it does fluctuate. There are some negatives and some positives, but I think the balance -- I've looked over the last number of years. I take the average, and it's a small positive, which is good.","length":386,"tagName":"p"},{"type":"text","content":"A lot of this change, though, was driven by our view on the rate of mortality improvement, which is as much driven by population trends as our own experience gains. So, yes, some elements will track our own experience gains, but a lot of this is general population trends in the UK. It's what led us to be very cautious.","length":320,"tagName":"p"},{"type":"text","content":"The trend was going the other way in the middle to late 2000s. So, I'd say 2005 to 2009 period, 2010 period, the trend was going the other way. And again, we reserved for that trend as we brought on large annuity books of business. And now we're seeing the population trend go the other way. So, it's less about our in-period experience gains and more about general trends.","length":373,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [86]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And, I mean, there's a dynamic, as Paul -- there is a dynamic that UK population mortality was not as strong as North American, and that we're seeing it sort of getting caught up to North American population mortality.","length":218,"tagName":"p"},{"type":"text","content":"And therefore, one would expect to see this moderating -- a moderation of improvement. And that's what sort of the actuarial profession would be observing and what we'd be seeing in statistics.","length":193,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Mario Mendonca, TD Securities - Analyst [87]","length":44,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And the nature of these questions is just to help build up my confidence level in the assumptions. So, when you do see the experience taper off in a particular quarter then I don't get too fussed by it. That's the nature of these questions.","length":240,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [88]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, I totally appreciate that.","length":30,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Mario Mendonca, TD Securities - Analyst [89]","length":44,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. The next thing I wanted to just quickly -- this is more numbers. You referred to experience not -- experience gains not being particularly strong. And Garry, I think the numbers were just coming at me too quickly. There was CAD75 million of investment gains, and then you said there were a number of things that would have offset that. Could you just help me think through what those number of things were again? I missed some of that.","length":441,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [90]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. I think it was about CAD64 million of other things. The largest of those I did call out was the CAD26 million in reinsurance mortality. And then the rest was spread across a number of factors. There was some pressure that I mentioned to group disability as well.","length":268,"tagName":"p"},{"type":"text","content":"And again, it's not that we didn't make money, it's just that we had anticipated an even higher contribution in our expected profit. So, in-quarter wasn't quite up to that expectation, so there's a downturn there in morbidity. I think overall across Ireland, the UK, and Canada, the morbidity would have been another CAD20 million, again this is Canadian pre-tax, and then some on policyholder behavior. And again, I think a lot of that is people taking actions on their policies before the -- in Canada before the tax changes come in 2017.","length":540,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [91]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, that's just a reminder that there's a change to the exempt test for -- on taxation of life policies. So, you'll probably note in the industry that there's elevated life sales across the industry, and that's driven by people trying to take advantage of a more favorable tax treatment prior to January 1, 2017 on cash value life insurance.","length":342,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Mario Mendonca, TD Securities - Analyst [92]","length":44,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you for going through that.","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [93]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you.","length":10,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [94]","length":13,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"(Operator instructions.) Darko Mihelic, RBC Capital Markets.","length":60,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Darko Mihelic, RBC Capital Markets - Analyst [95]","length":49,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Hi there. I have a number of questions, a couple of them just numbers. I wonder if I can just clarify a few things. Did I hear in part of the remarks that there was a CAD42 million difference in performance fees at Putnam?","length":222,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [96]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I believe in Garry's remarks he would have referenced CAD42 million as the combination of a number of different factors. But Garry, I'll let you clarify that.","length":158,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [97]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, that was a comment on the source of earnings display at Putnam. The year-over-year change in the expected profit line for Putnam was CAD42 million. And again, that's pre-tax in Canadian.","length":191,"tagName":"p"},{"type":"text","content":"Some of that would have been performance fees, but the lion's share of that was lower retail AUM, notwithstanding it's a higher institutional AUM. So, it's just the mix of fees.","length":177,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Darko Mihelic, RBC Capital Markets - Analyst [98]","length":49,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. All right. That does help, because I'm looking at page 42 of your supplement. And that's where I think I see that performance fees are negative again for the third quarter in a row, which leads to my real question, which is I think you guys suggest that you're still -- there's something like 68% of the fund still performing above the median over the five year. But I noticed that you actually left out the -- how the funds are -- how many of your funds are performing over the median for the three year. So, given that you're having lower performance fees, should we be concerned about the performance of the Putnam funds?","length":630,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [99]","length":56,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"You know what? First of all, I'd just characterize the fact that there's the concept of fulcrum fees, that when it's underperformance, you do -- relative to a target or a benchmark, you pay back. And then you get -- versus you can get benefits when you're overperforming. But I'm going to let Bob speak to the Putnam performance at this point. Bob?","length":348,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [100]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. Thank you, Paul. Short term performance over the last 12 months has been average. And I think the three year number in total, all products, above median is 44%.","length":165,"tagName":"p"},{"type":"text","content":"So, that reflects a performance shortfall in recent periods. But again, I think it's for a short period of time. And the five year number remains at 68% above median.","length":166,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Darko Mihelic, RBC Capital Markets - Analyst [101]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. And then just switching gears, the question with respect to Canada. We're noticing that your strain is high and higher in Canada. A question is what I'm interested in is, based on everything you know now and knowing that we have a tax change coming in the new year, should we continue -- should we expect a continued growth in strain, or is there a possibility here that strain should drop off materially in 2017? And I really care about 2017. I don't really care so much about next quarter.","length":497,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [102]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"So, let me speak to this, the dynamic of the business, and I'll let Stefan add a little bit more color.","length":103,"tagName":"p"},{"type":"text","content":"There's no doubt elevated strain right now because there's elevated sales, which is one factor. The second factor is the declining interest rates. And we tend to be very active on replacing as interest rates fall.","length":213,"tagName":"p"},{"type":"text","content":"But because of this exempt test work that we had to do, which, frankly, was investing in brand new redesigned products right across the piece, and it's something the industry had to do, we had to defer a price increase that we would have done sooner; we would have done probably late Q3 or even mid Q3 in favor of making sure that we had those products in place.","length":362,"tagName":"p"},{"type":"text","content":"So, our expectation as we go into the beginning of the new year is we will go through a repricing on related products where we're seeing that strain, and we'll get things back on track.","length":185,"tagName":"p"},{"type":"text","content":"Stefan, anything else you'd add?","length":32,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Stefan Kristjanson, Great-West Lifeco Inc. - President, COO Canada [103]","length":72,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, I think you've captured it well, Paul. And now that the tax exempt work is behind us, that actuarial team is turning their attention to that. So, we will be actively working on that repricing in the first quarter of 2017.","length":225,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Darko Mihelic, RBC Capital Markets - Analyst [104]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay, that's helpful. And then just one last question. As I looked through the supplementary, I noticed in the -- I don't ask questions about this too often because I don't see this too often. I don't think I've ever actually seen this. But if you look at page 31, the participating account is actually in a loss position. Should we care about that?","length":349,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [105]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I'll let Garry speak to that one.","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [106]","length":58,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"The short answer is no. In terms of participating account, this is a -- there is some strain on certain of the products when they're writing some of the large cases. But it's nothing that would flow through. It doesn't flow through into the shareholder account in any meaningful way.","length":283,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [107]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And interestingly, it's kind of the same dynamic, Darko, that I just referred to, which is we are in the process of redesigning products, including those prior products in relation to this exempt test.","length":201,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Darko Mihelic, RBC Capital Markets - Analyst [108]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Right.","length":6,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [109]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"So, some of that strain that we're experiencing will not be present as we go into the new year with the redesigned product.","length":123,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Darko Mihelic, RBC Capital Markets - Analyst [110]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay, that's as I suspected. Okay, thanks very much. Appreciate that.","length":69,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [111]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you.","length":10,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [112]","length":14,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets.","length":35,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [113]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, thanks very much. Good afternoon. Just a couple question with respect to Empower. The participants continue to generally be flat, the number of participants. And I think you had suggested that over the medium term we'd be getting up to in the area of 10 million participants, but we've been sort of stuck at 8 here for a while. What's causing that and what do you see as trying to grow those participants? And then just a couple follow ups.","length":445,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [114]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. Well, as a starting point, I think we sort of identified 10 as a medium term goal. And I think a lot of that's due to that incremental growth was going to be, once we could get the acquisition behind us, we'd be getting back to both accelerated organic growth but also thinking about other acquisitions, perhaps more targeted smaller entities.","length":348,"tagName":"p"},{"type":"text","content":"But I think generally speaking our participant levels and growth are consistent with what our expectations were right now. I'll let Bob speak to what the things that -- what things will occur that are going to get that participant growth going. Bob?","length":249,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [115]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. I would say we finished 2015 at 7.5 million, and 12-31-16 we're going to be at 8.1 million. So, that is significant growth on the platform.","length":144,"tagName":"p"},{"type":"text","content":"And the pipeline remains very, very strong. And we just have not had what we call a mega client close this year, but we have committed sales in 2017 of CAD4.4 billion, which is around 400,000 participants.","length":205,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [116]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And that's in the pipeline. Is that what you're saying?","length":55,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [117]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That's already been committed.","length":30,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [118]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. So, how should I look at the fact that the revenues are actually down 3% quarter-over-quarter in US dollars and that earnings before tax are down 15% quarter-over-quarter in Empower on a US dollar basis? What's happening here?","length":232,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [119]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Well, part of the revenue side of the equation depends upon the market, both equity and fixed income. And then on the cost basis, I think Garry referred to it. We're still in the integration, and we were -- spent money on integration this year and the enhancements to the system. So, that drove the change.","length":306,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [120]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"The investment in Empower spends were -- the investment in special Empower spends actually declined quarter-over-quarter. Is there just an uptick in general maintenance of this platform, or what would be -- is there anything more you can speak to?","length":247,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [121]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, it's more in the integration itself, preparing to bring these new clients onto the platform.","length":96,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [122]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. So, these aren't expenses associated with this CAD150 million enhancement spend then. These are just other expenses associated with bringing these new clients onboard. Is that correct?","length":190,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [123]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That is correct.","length":16,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [124]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay.","length":5,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [125]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. And Bob, one of the things -- and I'll let you add color to this. But one of the challenges is, until we're done, we are carrying the weight of multiple systems. So, we continue to pay for the cost of supporting and servicing the JP Morgan system until we're off that system. And we carry that on kind of in parallel with the build out and the -- carrying the weight of the new -- our invented Empower system.","length":414,"tagName":"p"},{"type":"text","content":"Once we get the integration over, we're going to shed those JP Morgan costs and we'll be able to begin to harvest the synergies we talked about. So, I think there's good opportunity there, but right now we're in that period of trying to get this deal closed out so we can shed the JP Morgan environment.","length":303,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [126]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, Paul. And just to add to that, we are operating two back ends. We want to get to one back end. But we're also operating three front ends now, and we want to get to one front end. Those projects are in front of us.","length":218,"tagName":"p"},{"type":"text","content":"And I would just add to the sales number in 2017. I touched on a number. In that number are four mega sales, the very large end of the market. So, the sales continue to do go well. And again, getting every -- the whole business on one platform is the key.","length":255,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [127]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. And then the tax rate on Empower it looks like is less than 10% just in this quarter. I know that this is small, but should we really think of the tax rate on Empower to be more in the 35% range?","length":201,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [128]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I wonder if Andra -- Bob or Andra might speak to that, or Garry, whoever?","length":73,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [129]","length":58,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, the tax rate would typically be more of the typical US tax rates that have been -- I think it's relatively modest. I think there was a small one-off this period.","length":166,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [130]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. And then one final question is really just with respect to the Canadian business. If we just look at the combination of sort of expected profit and strain and earnings on surplus, so we're not talking about all this changes in assumptions and experience gains and all that kind of stuff. I mean, that's down 7% year-over-year. It's down 4% quarter-over-quarter.","length":367,"tagName":"p"},{"type":"text","content":"Is there anything that's driving that? Have you changed your outlook with respect to some of these businesses, or is the business production -- just help me understand what's happening here that's contributing to that, or is it -- because we've had fairly good equity markets quarter-over-quarter as well. I'm just trying to figure out why they haven't really -- why this number isn't up more.","length":393,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [131]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry?","length":6,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [132]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Or why isn't this number up at all? Why is it down?","length":51,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [133]","length":58,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I'm just trying to cull from the start of your question, Tom. If you'd adjusted in there for currency, which was quite a (multiple speakers).","length":141,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [134]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I'm in Canada only though. I'm only looking at Canada.","length":54,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [135]","length":58,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I didn't hear the very start of the question. That's what I was asking. It's a Canada only question.","length":100,"tagName":"p"},{"type":"text","content":"In terms of -- I think the strain we've covered off earlier. It's with the combination of lower interest rates. The not repricing has been our normal course. That's certainly been a factor.","length":189,"tagName":"p"},{"type":"text","content":"The investment -- if I look at the investment gain contribution, while it was good this quarter, relative to the two comparative quarters the investment gains, in the experience gains section anyway, would be lower than they had been in those comparators.","length":255,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [136]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, I'm not -- I'm only -- or just what about expected profit? I mean, that's still down year-over-year. Is there anything that's driving that then? Maybe I'll just take that one line item.","length":190,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [137]","length":58,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"There were two elements to that. One is we were expecting a combination of higher expenses and tighter margins in the wealth business. So, expected profit year-over-year was down, I want to say about CAD10 million, CAD9 million to CAD10 million in the wealth business.","length":268,"tagName":"p"},{"type":"text","content":"And then we had adjusted our expectations somewhat for the group morbidity. And just the time taking to get the pricing action in, although it wasn't as strong a factor in this quarter, but we had adjusted that in the prior one.","length":228,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [138]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay, that's helpful.","length":21,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [139]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"So, I think two -- Tom, two things. One is we're on top of the pricing for group LTD, and we'll be on top of it on UL when we get into the new year.","length":148,"tagName":"p"},{"type":"text","content":"And then the other thing where -- you'll see a decline in Canadian expenses. We're on top of expenses in Canada, thinking in terms of some of the headwinds and pressure we've got with some tightening of margins in their wealthy businesses. You got to make sure you get at your cost structure.","length":292,"tagName":"p"},{"type":"text","content":"So, those are going to be some themes we'll talk about as we get into the new year.","length":83,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Tom Mackinnon, BMO Capital Markets - Analyst [140]","length":50,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Great. Thanks.","length":14,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [141]","length":14,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC.","length":18,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [142]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you. Good afternoon. I have a couple follow up questions on Empower. The first is with respect to some of the large client mandates you won with sales two, three, four quarters ago. Wondering if you can provide a status on the on-boarding of those clients, i.e., are we starting to see that flow through to revenue in Q3 and is there more of that to come in future quarters?","length":380,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [143]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. That first question, I'll turn that one to Bob and potentially Andra in support of Bob.","length":93,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [144]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. When you onboard a client, you go through an implementation integration that lasts anywhere from six to 12 months. And during that time, there is no revenue. So, once a client goes live, the revenue stream starts.","length":218,"tagName":"p"},{"type":"text","content":"And the clients that I talked about in implementation that have already been sold for 2017, those are costs items now, not revenue. So, there's always a lag from when you sell a client and when it goes live and when the revenue stream starts.","length":242,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [145]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob, I think Paul's question, though, related to clients we may have sold back in 2015 or early 2016. And I think your point would be that there is that six to 12 month lag, but some of those should be -- we'll have some of those implementations behind us soon. Would that be --?","length":279,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [146]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Absolutely. Absolutely. And the point I was making, that they usually have a six to 12 month implementation. And once they're done, that's when the revenue stream starts.","length":170,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [147]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Correct. So, the message I'm hearing is that revenue didn't go live for the most part in Q3. So, it's still something to hit the P&amp;L going forward.","length":151,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [148]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"That is correct.","length":16,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [149]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay, good. And then next question regarding Empower is I think I would expect, and I believe you communicated this as well as your investor day, that there might be more consolidation taking place, particularly with the DOL fiduciary rules. So, wondering if you're seeing any more indications of increased transaction activity or transaction interest, and if you would be willing to execute on such before you complete your system integration with the JP Morgan business.","length":472,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [150]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, we --.","length":11,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [151]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, Bob, you take it.","length":22,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [152]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. I'm sorry, Paul.","length":21,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [153]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"No, you go.","length":11,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [154]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"We are seeing a pick up in activity, not a large pick up but there are more opportunities out there.","length":100,"tagName":"p"},{"type":"text","content":"I would say as far as our appetite right now, we would be interested in a somewhat smaller acquisition, one that deals with smaller clients that don't have the customization that large clients do. So, we would not be focused on the large client market until after we get this integration finished, which should be done in February 2017.","length":336,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [155]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"And Bob, it would be fair to say that as kind of the DOL rule becomes a little bit clearer, because there's still lots of lack of clarity there, I think the market will sort of take that in. And you might expect to see a bit of an accelerated pace in terms of consolidation, because we do believe the market will consolidate for sure.","length":334,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Bob Reynolds, Great-West Lifeco Inc. - President, CEO Great-West Lifeco US [156]","length":80,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. I think what's going to drive the market consolidation is just the whole scale issue, right? I think the fiduciary rule does touch recordkeepers some, but this consolidation is going to be totally driven on the need for scale.","length":231,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [157]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Got it. Okay, thank you. That's helpful. Next question is if we could try to get into the expense growth a little bit more on the wealth management side. Wondering if that's coming more on the group business versus individual or if it's coming on both, and kind of what the nature of that expense growth is.","length":307,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Stefan Kristjanson, Great-West Lifeco Inc. - President, COO Canada [158]","length":72,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"From a Canada perspective, it's a fairly modest number at about CAD5 million of expense growth focused on technology spend to better build capabilities in the space. And so --.","length":176,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [159]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes. And I think we outlined that at investor day, that part of this is to think about not so much the clients of today but, as we look at segmentation, how are we going to reach and serve clients, ranging all the way from direct digital to kind of on the channel to the fully advised where we tend to be fairly focused. So, there's investment there, and that's where it would be.","length":380,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [160]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. Okay. And then excess capital and the ability to put that excess capital to work was a big message from your investor day. With the initial details out around LICAT, wondering if that message changes at all. Not looking for number, but maybe any kind of color you can provide on any potential pressure points from LICAT or a change of view on the excess capital story.","length":374,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [161]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"I'll let Garry speak to that one for sure.","length":42,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Garry MacNicholas, Great-West Lifeco Inc. - EVP, CFO [162]","length":58,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes, certainly. We just -- and so people are aware, the LICAT guidelines came out in September, but there is continuing activity of the industry, ourselves included, working with the -- with OSFI through various test runs to inform their final calibrations.","length":257,"tagName":"p"},{"type":"text","content":"But OSFI has indicated repeatedly that the overall industry trend -- they're not expecting to change capital levels at the overall industry basis. It's just more risk-based as to where that capital arises. And we would support that.","length":232,"tagName":"p"},{"type":"text","content":"While we'll have to wait for the final calibrations, of course, to be more exact, we're not expecting any fundamental changes to the -- or impact on our strategy at this point.","length":176,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [163]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Yes.","length":4,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [164]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Good. Okay. That's all that --.","length":31,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [165]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"We could still have -- fair to say we still have the appetite and we have capacity.","length":83,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Holden, CIBC - Analyst [166]","length":33,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Okay. Okay. That's all the questions I had. Thank you.","length":54,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [167]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Well, thank you very much.","length":26,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [168]","length":14,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you. This is the end of the question and answer session. I would now like to turn the meeting back to Mr. Paul Mahon.","length":123,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Paul Mahon, Great-West Lifeco Inc. - President, CEO [169]","length":57,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Good. So, I would like to thank everyone who participated today, and thank you for your questions. We look forward to reconnecting with you. I guess it'll be early February when we report on our Q4 results. And between now and then, I hope everybody has a good holiday season. Take care.","length":287,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Operator [170]","length":14,"tagName":"p"},{"type":"text","content":"--------------------------------------------------------------------------------","length":80,"tagName":"p"},{"type":"text","content":"Thank you. The conference has now ended. Please disconnect your lines at this time. And we thank you for your participation.","length":124,"tagName":"p"}],"summary":"Q3 2016 Great-West Lifeco Inc Earnings Call","tagsInfo":{},"site":"finance","se