        if (typeof Keystone !== 'undefined') {
            Keystone.pageMetaData = { contentType: 'watchlist' };
            Keystone.onLibraryReady('jQuery', function() {
                $(function() {
                    if ($.cookie("RGIS")) {
                        var cleanId = Keystone.UPM.Auth.parseRgis();
                        //check if the number is positive or negative
                        if(parseInt(cleanId, 10) > 0) {
                            $("#watchlistData").show();
                        }
                    }
                });
            });
        }
        var moved=false
        function checkForBigWatchlist(){
            var $watchlist=$('#watchlist');
            var widthOfOuter=($('#outmost-container').width());
            var $leftRail = $('#leftRail');
            if (widthOfOuter>1780) {
                if (!$watchlist.hasClass("bigwatchlist")) {
                    {
                        if ($leftRail.length > 0) {
                            document.getElementById('mainContent').className = "col-md-12 col-xs-12 col-lg-12 col-xl-12 col-sm-12";
                        }
                        var ua = window.navigator.userAgent;
                        var edge = ua.indexOf('Edge/');
                        if (edge==-1) {
                            $watchlist.addClass("bigwatchlist");
                        }
                        $watchlist.show();
                    }
                }
            }
            else {
                if ($watchlist.hasClass("bigwatchlist")) {
                    if ($leftRail.length > 0) {
                        $watchlist.removeClass("bigwatchlist");
                        $watchlist.addClass("leftWathclist");
//                            $watchlist.detach().appendTo(leftRail);
                        document.getElementById('mainContent').className = "col-md-12 col-xs-12 col-lg-10 col-xl-10 col-sm-12";
                        document.getElementById('leftRail').className = "col-lg-2 col-xl-2 hidden-md hidden-sm hidden-xs";
                    }
                    else{
                        $watchlist.removeClass("bigwatchlist");
                    }
                }
                else{
                    if ($leftRail.length > 0) {
                        $watchlist.removeClass("bigwatchlist");
                        $watchlist.addClass("leftWathclist");
                        document.getElementById('mainContent').className = "col-md-12 col-xs-12 col-lg-10 col-xl-10 col-sm-12";
                        document.getElementById('leftRail').className = "col-lg-2 col-xl-2 hidden-md hidden-sm hidden-xs";

                    }
                    $watchlist.show();
                }
            }
            $watchlist.show();
        }
        if (typeof Keystone !== 'undefined') {
            Keystone.pageMetaData = {contentType: 'watchlist'};
            Keystone.onLibraryReady('jQuery', function () {
                $( window ).load(function() {

                        checkForBigWatchlist();

                    $( window ).resize(_.throttle(function() {
                        checkForBigWatchlist();
                    }, 400));
                });
            });
        }
    </script>
