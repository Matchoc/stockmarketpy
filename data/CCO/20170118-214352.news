                        <button class="facebook"><img src="{{resources.content}}/svg/icons/ico-facebook-white.svg" alt="Share on Facebook" /></button>
                    </div>
                {{/link}}
            </div>
        {{/Bulletin}}
    {{/closed}}
    {{/Expired}}
{{/bulletins}}</script>

