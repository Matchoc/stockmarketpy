	<script type="text/javascript">

	/* Class manager for handling a survey */
	var MRSurveyManager = function (args) {

		var self = this;

		self.args = args;
		self._submissionData = null;
		self._isViewed = null;

		self.isDisplayPending = false;

		//Init the survey after document has loaded - Check display requirements and display if possible
		self.init = function () {

			if (jQuery('html').hasClass('small-width')) {
				return;
			}

			if (self.isViewed() || self.isCompleted()) {
				return;
			}

			self.pushView();

			//Handle collapse+expand of the modal
			jQuery(self.args.targetElement).find('.mr-form-dismiss').click(function () {

				self.toggleCollapseModal();
			});

			// Handle survey progression and submission
			jQuery(self.args.targetElement).find('form').find('input').click(function () {

				var input = jQuery(this);
				var group = ( jQuery(this).attr('data-group') ) ? '.group-' + jQuery(this).attr('data-group') : '';

				if (input.closest('.mr-form-question').siblings('.mr-form-question' + group).get(0)) {

					input.closest('.mr-form-question').hide().siblings('.mr-form-question' + group).show();

				} else {

					self.submit();
				}

			});
		};

		self.pushView = function () {

			if (self.isViewed() || self.isCompleted()) {
				return;
			}

			if (self.getViews() >= self.args.displayRules.pageViews && !self.isDisplayPending) {

				self.isDisplayPending = true;

				setTimeout(function () {
					self.displayModal();
					self.resetViews();
					self.setIsViewed(true);
				}, ( self.args.displayRules.timeOnPage * 1000 ));

			} else {
				self.incrementViews();
			}
		}

		self.toggleCollapseModal = function () {

			jQuery(self.args.targetElement).find('.mr-form-min').toggle();
			jQuery(self.args.targetElement).find('.mr-form-max').toggle();
			jQuery(self.args.targetElement).find('form').toggle();

		};

		self.submit = function () {

			var data = self.getFormData();

			self.setSubmissionData(data);

			data['survey-name'] = self.args.name;

			MR_User_Tracking.trackEvent('Survey Submitted', data);

			jQuery(self.args.targetElement).find('form').hide();
			jQuery(self.args.targetElement).find('.mr-form-complete').show();

			setTimeout(function () {

				jQuery(self.args.targetElement).hide();

			}, 1500);
		};

		self.displayModal = function () {

			jQuery(self.args.targetElement).show();
		};

		self.hideModal = function () {

			jQuery(self.args.targetElement).hide();
		};

		self.getViews = function () {

			return self.getCookie(self.args.cookieName + '_view_count');
		};

		self.setViews = function (value) {

			self.setCookie(self.args.cookieName + '_view_count', value, 999);
		};

		self.resetViews = function () {

			self.setViews(0);
		};

		self.incrementViews = function () {

			var count = self.getViews();

			if (!count)
				count = 0;

			self.setViews(++count);
		};

		self.isViewed = function () {

			if (self._isViewed !== null) {
				return self._isViewed;
			}

			return self.getCookie(self.args.cookieName + '_is_viewed');
		};

		self.setIsViewed = function (bool) {

			self._isViewed = bool;

			self.setCookie(self.args.cookieName + '_is_viewed', bool, 14);
			self.resetViews();
		};

		self.isCompleted = function () {

			return ( self.getSubmissionData() );
		};

		self.setSubmissionData = function (completedValue) {

			self.resetViews();
			self.setCookie(self.args.cookieName + '_is_completed', completedValue, 999);
			self._submissionData = null;
		};

		self.getSubmissionData = function () {

			if (self._submissionData !== null) {
				return self._submissionData;
			}

			return self._submissionData = self.getCookie(self.args.cookieName + '_is_completed');
		};

		self.getCookie = function (cookieName) {

			if (document.cookie.length > 0) {
				c_start = document.cookie.indexOf(cookieName + "=");

				if (c_start != -1) {
					c_start = c_start + cookieName.length + 1;
					c_end = document.cookie.indexOf(";", c_start);

					if (c_end == -1) {
						c_end = document.cookie.length;
					}

					return JSON.parse(unescape(document.cookie.substring(c_start, c_end)));
				}
			}
			return "";
		};

		self.setCookie = function (cookieName, cookieValue, cookieDays) {

			cookieValue = JSON.stringify(cookieValue);

			if (cookieDays) {
				var date = new Date();
				date.setTime(date.getTime() + ( cookieDays * 24 * 60 * 60 * 1000 ));
				var expires = "; expires=" + date.toGMTString();
			} else {
				var expires = "";
			}

			document.cookie = cookieName + "=" + cookieValue + expires + "; path=/";

		};

		self.getFormData = function () {

			var paramObj = {};

			jQuery.each(jQuery(self.args.targetElement).find('form').serializeArray(), function (_, kv) {
				if (paramObj.hasOwnProperty(kv.name)) {
					paramObj[kv.name] = $.makeArray(paramObj[kv.name]);
					paramObj[kv.name].push(kv.value);
				}
				else {
					paramObj[kv.name] = kv.value;
				}
			});

			return paramObj;
		}
	};

	//Don't bother loading the surveys if JSON is not available
	if (typeof( JSON ) !== 'undefined' && typeof( JSON.stringify ) !== 'undefined') {

		//Surveys data goes here
		var MRSurveysData = [
			{
				name: 'investor-type-survey',
				targetElement: '.investor-type-survey',
				cookieName: 'investor_type_survey',
				displayRules: {pageViews: 3, timeOnPage: 10}
			}
		];

		//Manage all surveys with instances of MRSurveyManager classes
		var MRSurveys = new function () {

			var self = this;
			self.surveys = [];

			self.getByName = function (name) {

				var survey = false;

				var index;

				for (index = 0; index < self.surveys.length; ++index) {

					if (name === self.surveys[index].args.name) {
						survey = self.surveys[index];
					}
				}

				return survey;
			}
		};

		var MRSurveysIndex;

		for (MRSurveysIndex = 0; MRSurveysIndex < MRSurveysData.length; ++MRSurveysIndex) {

			var survey = new MRSurveyManager(MRSurveysData[MRSurveysIndex]);

			MRSurveys.surveys.push(survey);
		}
	}

</script>
	

