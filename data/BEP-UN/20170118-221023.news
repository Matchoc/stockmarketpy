    <script>
        var socialShare = function(network) {
            var url = encodeURI(window.location.href);
            var title = document.title;
            var twitter_title = encodeURIComponent($('#twitter_title').text());
            var shareUrls = [];
            shareUrls['Facebook'] = "https://facebook.com/sharer/sharer.php?u=" + url;
            shareUrls['Twitter'] = "https://twitter.com/intent/tweet/?text=" + twitter_title + "&url=" + url;
            shareUrls['Google+'] = "https://plus.google.com/share?url=" + url;
            shareUrls['Tumblr'] = "http://tumblr.com/widgets/share/tool?canonicalUrl=" + url + '&shareSource=tumblr_share_button';
            shareUrls['mailto'] = "mailto:?subject=" + title + "&body=" + url;
            shareUrls['LinkedIn'] = "https://www.linkedin.com/shareArticle?mini=true&url=" + url + "&title=" + title + "&summary=" + title + "&source=" + url;

            if (network != "mailto"){
                analytics.ready(function() {
                    ga('send', {
                      hitType: 'social',
                      socialNetwork: network,
                      socialAction: 'like',
                      socialTarget: window.location.href.toString()
                    });
                });
            }

            if (typeof(shareUrls[network]) !== "undefined") {
                window.location = shareUrls[network];
            }
        };
    </script>


